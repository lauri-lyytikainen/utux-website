{
  "name": "utux-website",
  "version": "1.0.0",
  "description": "Custom website for Utux",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.0",
    "@payloadcms/db-postgres": "3.31.0",
    "@payloadcms/drizzle": "^3.31.0",
    "@payloadcms/email-nodemailer": "^3.31.0",
    "@payloadcms/next": "3.31.0",
    "@payloadcms/payload-cloud": "3.31.0",
    "@payloadcms/plugin-nested-docs": "^3.31.0",
    "@payloadcms/plugin-seo": "^3.31.0",
    "@payloadcms/richtext-lexical": "3.31.0",
    "@payloadcms/storage-s3": "^3.31.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@tailwindcss/postcss": "^4.0.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "lucide-react": "^0.479.0",
    "next": "15.2.4",
    "next-i18n-router": "^5.5.1",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.0",
    "payload": "3.31.0",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-cookie-manager": "^3.4.2",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.12",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.2.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "supportedArchitectures": {
      "os": [
        "win32",
        "linux"
      ],
      "cpu": [
        "x64"
      ],
      "libc": [
        "musl",
        "glibc"
      ]
    }
  }
}
